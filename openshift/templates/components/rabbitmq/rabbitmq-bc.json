{
    "kind": "Template",
    "apiVersion": "v1",
    "metadata": {
        "name": "rabbitmq-bc",
        "creationTimestamp": null
    },
    "parameters": [
        {
            "name": "ADMIN_PASSWORD",
            "from": "[a-zA-Z0-9]{16}",
            "generate": "expression",
            "required": true
        },
        {
            "name": "TFRS_PASSWORD",
            "from": "[a-zA-Z0-9]{16}",
            "generate": "expression",
            "required": true
        }
    ],
    "objects": [
        {
            "kind": "BuildConfig",
            "apiVersion": "v1",
            "metadata": {
                "name": "rabbitmq-tfrs",
                "creationTimestamp": null
            },
            "spec": {
                "triggers": [],
                "runPolicy": "Serial",
                "source": {
                    "type": "Dockerfile",
                    "dockerfile": "FROM rabbitmq:3.7.7-management\nRUN apt-get update\nRUN apt-get install -y gettext-base\nARG ADMIN_PASSWORD\nARG TFRS_PASSWORD\nRUN echo \"\" | base64 -d | gunzip - | envsubst \u003e /etc/rabbitmq/definitions.json"
                },
                "strategy": {
                    "type": "Docker",
                    "dockerStrategy": {
                        "env": [
                            {
                                "name": "ADMIN_PASSWORD",
                                "valueFrom": {
                                    "secretKeyRef": {
                                        "name": "rabbitmq-tfrs",
                                        "key": "ADMIN_PASSWORD"
                                    }
                                }
                            },
                            {
                                "name": "TFRS_PASSWORD",
                                "valueFrom": {
                                    "secretKeyRef": {
                                        "name": "rabbitmq-tfrs",
                                        "key": "TFRS_PASSWORD"
                                    }
                                }
                            }
                        ]
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "rabbitmq-tfrs:latest"
                    }
                },
                "resources": {},
                "postCommit": {},
                "nodeSelector": null
            },
            "status": {
                "lastVersion": 0
            }
        },
        {
            "kind": "Secret",
            "apiVersion": "v1",
            "metadata": {
                "name": "rabbitmq-tfrs",
                "creationTimestamp": null
            },
            "stringData": {
                "ADMIN_PASSWORD": "${ADMIN_PASSWORD}",
                "TFRS_PASSWORD": "${TFRS_PASSWORD}"
            }
        },
        {
            "kind": "ImageStream",
            "apiVersion": "v1",
            "metadata": {
                "name": "rabbitmq-tfrs",
                "generation": 1,
                "creationTimestamp": null
            },
            "spec": {
                "tags": [
                    {
                        "name": "latest",
                        "annotations": null,
                        "from": {
                            "kind": "DockerImage",
                            "name": "docker-registry.default.svc:5000/mem-tfrs-tools/rabbitmq-tfrs:latest"
                        },
                        "generation": null,
                        "importPolicy": {},
                        "referencePolicy": {
                            "type": ""
                        }
                    }
                ]
            },
            "status": {
                "dockerImageRepository": ""
            }
        }
    ]
}
