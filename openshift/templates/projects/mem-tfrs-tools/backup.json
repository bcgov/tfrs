{
    "kind": "Template",
    "apiVersion": "template.openshift.io/v1",
    "metadata": {
        "name": "backup",
        "creationTimestamp": null
    },
    "objects": [
        {
            "kind": "ImageStream",
            "apiVersion": "image.openshift.io/v1",
            "metadata": {
                "name": "backup",
                "generation": 2,
                "creationTimestamp": null
            },
            "spec": {
                "lookupPolicy": {
                    "local": false
                },
                "tags": [
                    {
                        "name": "dev",
                        "annotations": null,
                        "from": {
                            "kind": "DockerImage",
                            "name": "docker-registry.default.svc:5000/mem-tfrs-tools/backup:dev"
                        },
                        "generation": 2,
                        "importPolicy": {},
                        "referencePolicy": {
                            "type": "Source"
                        }
                    },
                    {
                        "name": "latest",
                        "annotations": null,
                        "from": {
                            "kind": "DockerImage",
                            "name": "docker-registry.default.svc:5000/mem-tfrs-tools/backup:latest"
                        },
                        "generation": null,
                        "importPolicy": {},
                        "referencePolicy": {
                            "type": ""
                        }
                    }
                ]
            },
            "status": {
                "dockerImageRepository": ""
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "build.openshift.io/v1",
            "metadata": {
                "name": "backup",
                "creationTimestamp": null,
                "labels": {
                    "app": "backup"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    }
                ],
                "runPolicy": "Serial",
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/bcgov/tfrs.git",
                        "ref": "devops-backup"
                    },
                    "contextDir": "/backup-container/docker"
                },
                "strategy": {
                    "type": "Docker",
                    "dockerStrategy": {
                        "from": {
                            "kind": "DockerImage",
                            "name": "registry.access.redhat.com/rhscl/postgresql-10-rhel7:latest"
                        },
                        "dockerfilePath": "Dockerfile"
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "backup:latest"
                    }
                },
                "resources": {},
                "postCommit": {},
                "nodeSelector": null,
                "successfulBuildsHistoryLimit": 5,
                "failedBuildsHistoryLimit": 5
            },
            "status": {
                "lastVersion": 0
            }
        }
    ]
}
